{
  "name": "trpc-electron",
  "description": "Electron support for tRPC",
  "version": "0.1.2",
  "exports": {
    "./main": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.mjs"
    },
    "./renderer": {
      "types": "./dist/renderer.d.ts",
      "require": "./dist/renderer.cjs",
      "import": "./dist/renderer.mjs"
    },
    "./preload": {
      "types": "./dist/preload.d.ts",
      "require": "./dist/preload.cjs",
      "import": "./dist/preload.mjs"
    }
  },
  "author": "Mat Sz <contact@matsz.dev>",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "main.d.ts",
    "renderer.d.ts"
  ],
  "scripts": {
    "build": "tsc && vite build -c src/main/vite.config.ts && vite build -c src/renderer/vite.config.ts && vite build -c src/preload/vite.config.ts && yarn build:types",
    "build:types": "dts-bundle-generator --config ./dts-bundle-generator.config.ts && yarn copy:types",
    "copy:types": "cp dist/main.d.ts dist/renderer.d.ts dist/preload.d.ts ./",
    "test": "vitest -c vitest.config.ts",
    "test:ci": "vitest run -c vitest.config.ts --coverage",
    "prepublishOnly": "corepack yarn build",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.83.0",
    "@trpc/client": "next",
    "@trpc/server": "next",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "builtin-modules": "^5.0.0",
    "dts-bundle-generator": "^9.5.1",
    "electron": "^37.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "superjson": "^2.2.2",
    "vite": "^7.0.4",
    "vite-plugin-commonjs-externals": "^0.1.4",
    "vitest": "^3.2.4",
    "zod": "^4.0.5"
  },
  "peerDependencies": {
    "@trpc/client": ">=11.0.0",
    "@trpc/server": ">=11.0.0",
    "electron": ">19.0.0"
  }
}